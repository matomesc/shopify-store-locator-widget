{
  "name": "neutek-locator-widget",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "start:server": "concurrently \"http-server ./build -p 3002\" \"watch 'yarn build:development && yarn listBuildFiles > ./build/files.txt' src\"",
    "build": "react-scripts build",
    "build:development": "env-cmd -f .env react-scripts build",
    "build:staging": "env-cmd -f .env.staging react-scripts build",
    "deploy:staging": "",
    "deploy:production": "",
    "listBuildFiles": "tsx ./src/bin/listBuildFiles.ts",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "commit": "lint-staged && cz"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/watch": "^1",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "http-server": "^14.1.1",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "watch": "^1.0.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@4.3.1"
}
